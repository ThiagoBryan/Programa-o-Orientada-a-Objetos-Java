package br.com.residencia;

public class Lista01treinarjava05 {

	public static void main(String[] args) {
		
		//5.O programa “Riuju” escreve na tela o resultado das expressões abaixo:
			//a. 			a.	2 + 3 - 5 * 8 - 4 + 354 - 521 + 7 * 66
			//b.			b.	2 + 7 * (14 - 21) + 28 * 3 * 42 + 740 - (156 + 4 + 40) * 9 
		
		
		System.out.println("\t===RIUJU===\n");
		
		double a = 2 + 3 - 5 * 8 - 4 + 354 - 521 + 7 * 66;
		double b = 2 + 7 * (14 - 21) + 28 * 3 * 42 + 740 - (156 + 4 + 40) * 9;
		
		System.out.println("Expressões : ");
		System.out.println("a = 2 + 3 - 5 * 8 - 4 + 354 - 521 + 7 * 66");
		System.out.println("b = 2 + 7 * (14 - 21) + 28 * 3 * 42 + 740 - (156 + 4 + 40) * 9\n");
		
		System.out.println("O resultado da expressão a é: " + a);
		System.out.println("O resultado da expressão b é: " + b);

	}

}
